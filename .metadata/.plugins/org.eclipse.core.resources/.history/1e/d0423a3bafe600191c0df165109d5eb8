#include<iostream>
#include<iomanip>

void timeConverter(double seconds);
using namespace std;
int main(){
	//call anything you want here
	timeConverter(30000);
}

void timeConverter(double seconds){
	double hours =  seconds/3600;
	double minutes = (seconds%3600)/60;
	double seconds_;



	cout<<"hours"<<hours<<endl;
	cout<<"minutes"<<minutes<<endl;
	cout<<"seconds"<<seconds<<endl;
}


