#include <iostream>
#include "Employee.h"
using namespace std;

void process(double *amount);

int main() {

	double attack = 100.5;

	cout<<&attack<<endl;
	process(&attack);

	return 0;
}

void process(double *amount){
	cout<<*amount<<endl;
	cout<<&amount<<endl;
	cout<<amount<<endl;
	cout<<&amount<<endl;
	cout<<amount<<endl;
}
