#include<iostream>
#include<iomanip>

void timeConverter(double seconds);
using namespace std;
int main(){
	//call anything you want here
	timeConverter(30000);
}

void timeConverter(double seconds){
	double hours =  seconds/3600;
	int minutes = hours/60;
	double seconds_=0;



	cout<<"hours"<<hours<<endl;
	cout<<"minutes"<<minutes<<endl;
	cout<<"seconds"<<seconds_<<endl;
}


